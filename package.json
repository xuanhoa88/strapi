{
  "private": true,
  "version": "1.0.0",
  "description": "An application created by KoaJS, inspired by Strapi",
  "author": "xuan.0211@gmail.com",
  "bin": {
    "strapi": "./packages/strapi.js"
  },
  "scripts": {
    "strapi": "node ./packages/strapi.js",
    "prod": "npm run strapi -- production",
    "dev": "npm run strapi -- develop",
    "env": "npm run strapi -- env",
    "generate": "npm run strapi -- generate",
    "migrate": "npm run strapi -- migrate",
    "seed": "npm run strapi -- seed",
    "lint": "node_modules/.bin/eslint './**/*.js' --fix",
    "pretty": "./node_modules/.bin/prettier --write 'packages/**/*.js' 'api/**/*.js' '.bin/**/*.js' 'data/**/*.js' 'hooks/**/*.js' 'config/**/*.js' 'plugins/**/*.js' 'test/**/*.js'",
    "test": "mocha",
    "docker:dev": "docker-compose -f ./docker/docker-compose.yml up --build",
    "docker:build": "cross-env NODE_ENV=production docker-compose -f ./docker/docker-compose.yml build"
  },
  "devDependencies": {
    "chakram": "1.5.0",
    "cross-env": "7.0.3",
    "eslint": "8.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.2",
    "mocha": "10.0.0",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "@koa/cors": "3.3.0",
    "@strapi/logger": "4.1.12",
    "@strapi/utils": "4.1.12",
    "adm-zip": "0.5.9",
    "async": "3.2.3",
    "bluebird": "3.7.2",
    "chalk": "2.4.1",
    "chokidar": "3.5.3",
    "cli-table3": "0.6.2",
    "commander": "9.3.0",
    "create-error": "0.3.1",
    "date-fns": "2.28.0",
    "debug": "4.3.4",
    "delegates": "1.0.0",
    "dotenv": "16.0.1",
    "exceljs": "4.3.0",
    "glob": "8.0.3",
    "i18n-2": "0.7.3",
    "inflection": "1.13.2",
    "joi": "17.6.0",
    "knex": "2.1.0",
    "koa": "2.13.4",
    "koa-body": "5.0.0",
    "koa-compose": "4.1.0",
    "koa-compress": "5.1.0",
    "koa-convert": "2.0.0",
    "koa-ip": "2.1.2",
    "koa-locale": "1.3.0",
    "koa-lusca": "2.2.0",
    "koa-router": "10.1.1",
    "koa-static": "5.0.0",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "mongoose": "6.3.6",
    "mongoose-float": "1.0.4",
    "mongoose-long": "0.5.1",
    "mysql": "2.18.1",
    "node-schedule": "2.1.0",
    "p-map": "4.0.0",
    "pg": "8.7.3",
    "pluralize": "8.0.0",
    "qs": "6.10.3",
    "reportback": "2.0.2",
    "resolve-cwd": "3.0.0",
    "rimraf": "3.0.2",
    "socket.io": "4.5.1",
    "sqlite3": "5.0.8",
    "uuid": "8.3.2",
    "verror": "1.10.1"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint",
      "./node_modules/.bin/prettier --write"
    ]
  }
}
