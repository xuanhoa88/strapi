{
  "extends": [
    "airbnb-base/whitespace",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-console": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-promise-executor-return": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  },
  "globals": {
    "strapi": true
  }
}